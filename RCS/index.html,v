function checkTime(i) {
    if (i<10) {i = "0" + i};  // add zero in front of numbers < 10
    return i;
}

// Our Data
ggItemList = [
 { h:"7", m:"35", s:"Get Dressed" },
 { h:"7", m:"45", s:"Brush Teeth" },
 { h:"7", m:"55", s:"Vitamin" },
 { h:"8", m:"00", s:"Shoes & Backpacks" },
 { h:"8", m:"05", s:"GO TO BUS" }
 //,{ h:"14", m:"07", s:"TEST" }
];
ggWeekDayStuff = {
  'Monday': "Caroline-Art,  Liam-Music",
  'Tuesday': "Caroline-Music, Liam-Library",
  'Wednesday': "Caroline-Gym, Liam-Gym",
  'Thursday': "Caroline-Computers & Gym, Liam-Gym",
  'Friday': "Caroline-Library, Liam-Computers & Art; Karate"
};

// Setup
w = [ "Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday" ];
m = [ "January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December" ]
t=new Date();
s = w[t.getDay()] + " " + t.getDate() + " " + m[t.getMonth()] + " " + t.getFullYear();
//console.log("s: " + s);
$("#date").html(s);
$("#weekdaystuff").html(ggWeekDayStuff[w[t.getDay()]]);

n = 0;
ggItemList.forEach( function(i) {
  t = i.h + ":" + i.m;
  i.currStyle = "ggWait";
  i.itemsecs = i.h * 60 * 60 + i.m * 60;
  i.itemid = "item" + n;
  i.progid = "prog" + n;
  i.progDivid = "progdiv" + n;
  n++;
  $("#ggItems").append("<div class='row'> \
        <div class='col-md-1'></div> \
        <div class='col-md-2'> \
          <div class='progress hidden' id='" + i.progDivid + "'> \
            <div class='progress-bar progress-bar-warning progress-bar-striped active' role='progressbar' style='width: 0%;' id='" + i.progid + "'> \
            </div> \
          </div> \
        </div> \
        <div class='col-md-2 ggItems ggWait " +i.itemid+ "'>" + t + "</div> \
        <div class='col-md-4 ggItems ggWait " +i.itemid+ "'>" + i.s + "</div> \
      </div>");

} );

