    'fileId': fileId
  });
request.execute(function(resp) {
  console.log('gg Title: ' + resp.title);
  console.log('gg Description: ' + resp.description);
  console.log('gg MIME type: ' + resp.mimeType);
  console.log('gg dload: ' + resp.downLoadUrl);
  });
}
