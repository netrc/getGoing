<script>
function checkSetStyle( i, newStyle ) {
    if (newStyle != i.currStyle) {
       $("."+i.itemid).removeClass(i.currStyle);
       i.currStyle = newStyle;
       $("."+i.itemid).addClass(i.currStyle);
    }
}
d355 1
a355 37
function startTime() {
    var tnow=new Date();
    var h=tnow.getHours();
    var m=tnow.getMinutes();
    var s=tnow.getSeconds();
    m = checkTime(m);
    s = checkTime(s);
    $("#time").html(h+":"+m);

    currsecs = h*60*60 + m * 60 + s*1;
    //console.log("cs: " + currsecs + " h" +h+ " m " +m+ " s " + s );

    n = 0;
    ggItemList.forEach( function(i) {
      //console.log("cs: " + currsecs + " " + i.itemid + " ... is: " + i.itemsecs );
      currDiff = currsecs - i.itemsecs;
      //console.log("cd="+currDiff);
      if (currDiff < -120) { // we're early 
        checkSetStyle( i, "ggWait");
      } else if (currDiff == -120) { // start pre-boarding
        $("#"+i.progDivid).removeClass("hidden").addClass("shown");
      } else if (currDiff < 0) { // we're pre-boarding
        checkSetStyle( i, "ggPre");

        w=Math.round((120+currDiff)*100/120);
        console.log("w="+w+"%");
        $("#"+i.progid).css("width",w+"%");
      } else if (currDiff == 0) {
        $("#"+i.progDivid).addClass("hidden");
      } else if (currDiff < 120) { // we're late
        //console.log(i.itemid + " is late");
        checkSetStyle( i, "ggLate");
      } else {
        checkSetStyle( i, "ggPostLate");
      }
      n++;
    });
